{
    "name": "@gipjazes/api",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "predev": "powershell -Command \"Get-NetTCPConnection -LocalPort 3000 -ErrorAction SilentlyContinue | ForEach-Object { Stop-Process -Id $_.OwningProcess -Force -ErrorAction SilentlyContinue }\"",
        "dev": "nodemon --exec ts-node src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "lint": "eslint src/**/*.ts",
        "db:migrate": "psql -d gipjazes -f ../../database/schema.sql"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.450.0",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.6",
        "dotenv": "^16.6.1",
        "express": "^4.22.1",
        "ffmpeg-static": "^5.2.0",
        "ffprobe-static": "^3.1.0",
        "fluent-ffmpeg": "^2.1.2",
        "jsonwebtoken": "^9.0.3",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.18.0",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.19",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.17.25",
        "@types/fluent-ffmpeg": "^2.1.24",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/multer": "^1.4.13",
        "@types/node": "^20.19.30",
        "@types/pg": "^8.16.0",
        "@types/uuid": "^9.0.8",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    }
}